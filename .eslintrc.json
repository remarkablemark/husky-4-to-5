{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["error"],
    "no-debugger": "error",
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
